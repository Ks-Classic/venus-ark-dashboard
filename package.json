{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "chcp 65001 && cross-env NODE_OPTIONS=--enable-source-maps next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:data-fetch": "node --loader ts-node/esm scripts/data-fetch-test.mts",
    "analyze:notion": "node scripts/analyze-notion-databases.js",
    "setup:env": "echo 'Please copy .env.example to .env.local and configure your environment variables'",
    "sync:recruitment": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/sync-recruitment-data.ts",
    "sync:notion": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/sync-notion-data.ts",
    "sync:all": "npm run sync:recruitment && npm run sync:notion",
    "check:data-quality": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/check-data-quality.ts",
    "check:duplicates": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/check-duplicates.ts",
    "check:missing-fields": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/check-missing-fields.ts",
    "generate:report": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/generate-weekly-report.ts",
    "generate:work-status": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/generate-work-status-report.ts",
    "generate:optimized-summaries": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/generate-optimized-weekly-summaries.ts",
    "create:sample-data": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/create-sample-data.ts",
    "db:init": "gcloud firestore databases create --region=asia-northeast1",
    "db:rules": "gcloud firestore rules deploy firestore.rules",
    "db:indexes": "gcloud firestore indexes create --index-file=firestore.indexes.json",
    "db:migrate": "npm run db:rules && npm run db:indexes",
    "db:backup": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/backup-firestore.ts",
    "db:restore": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/restore-firestore.ts",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "debug:get-headers": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/temp_get_sheet_headers.ts",
    "diag:form-matching": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/diagnose-form-matching.ts",
    "env:validate": "varlock load",
    "env:check": "varlock load --dry-run",
    "dev:env": "varlock run -- npm run dev",
    "env:validate:custom": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json scripts/validate-env.ts"
  },
